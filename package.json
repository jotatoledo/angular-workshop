    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6",
    "tslint-config-prettier": "^1.17.0"
  }
