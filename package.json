{
  "name": "angular-workshop",
  "version": "1.2.4",
  "license": "MIT",
  "scripts": {
    "start": "ng serve",
    "start-aot": "ng serve --aot",
    "start-paot": "ng serve -p --aot",
    "build": "ng build --aot --prod --env=prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ghpages": "ng build --base-href /angular-workshop/ -prod --aot && ngh",
    "deploy": "ng build --aot -p",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -p angular",
    "format": "prettier --write --config ./.prettierrc.json --list-different ./src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss,.html}"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jotatoledo/angular-workshop.git"
  },
  "author": {
    "email": "jotatoledon@gmail.com",
    "name": "Jose Toledo Navarro",
    "url": "https://github.com/jotatoledo"
  },
  "dependencies": {
    "@angular/animations": "^7.1.4",
    "@angular/cdk": "^7.2.0",
    "@angular/common": "^7.1.4",
    "@angular/compiler": "^7.1.4",
    "@angular/core": "^7.1.4",
    "@angular/flex-layout": "7.0.0-beta.22",
    "@angular/forms": "^7.1.4",
    "@angular/material": "^7.2.0",
    "@angular/platform-browser": "^7.1.4",
    "@angular/platform-browser-dynamic": "^7.1.4",
    "@angular/router": "^7.1.4",
    "@covalent/core": "^2.0.0",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "rxjs": "^6.3.3",
